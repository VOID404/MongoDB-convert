{
  "name": "mongodb-convert",
  "version": "1.0.0",
  "description": "Copies and transforms collections from all mongodb databases on given servers and upload them to another server.",
  "main": "app.js",
  "scripts": {
    "test": "env travis=false node_modules/.bin/mocha",
    "travistest": "env travis=true node_modules/.bin/mocha ",
    "start": "node app.js",
    "readme": "rm README.md && node_modules/.bin/readme-generator > README.md"
  },
  "author": "Wojciech Nawa",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/VOID404/MongoDB-convert"
  },
  "preferGlobal": true,
  "private": true,
  "dependencies": {
    "JSON": "^1.0.0",
    "async": "^2.0.0",
    "mongodb": "^2.2.1",
    "synchronize": "^0.9.15"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chance": "^1.0.3",
    "mocha": "^2.5.3",
    "readme-generator": "git+https://github.com/VOID404/readme-generator.git"
  },
  "readMeGenerator": {
    "template": "readmeTemplate.md",
    "licenseFile": "LICENSE",
    "shields": {
      "travis": "[![Travis](https://img.shields.io/travis/VOID404/MongoDB-convert.svg?maxAge=2592000&style=flat-square)](https://travis-ci.org/VOID404/MongoDB-convert)",
      "github-open-issues": "[![GitHub issues](https://img.shields.io/github/issues/VOID404/MongoDB-convert.svg?maxAge=2592000&style=flat-square)](https://github.com/VOID404/MongoDB-convert/issues)",
      "node-version": "[![Node version](https://img.shields.io/badge/node-4.4.7-brightgreen.svg?style=flat-square)](https://nodejs.org/en/)",
      "mongodb-version": "[![Mongodb version](https://img.shields.io/badge/mongodb-3.2.7-brightgreen.svg?style=flat-square)](https://www.mongodb.com/download-center?jmp=nav#community)",
      "license": "[![license](https://img.shields.io/badge/license-GPL--3.0-brightgreen.svg?style=flat-square)](https://raw.githubusercontent.com/VOID404/MongoDB-convert/master/LICENSE)",
      "github-watchers": "[![GitHub watchers](https://img.shields.io/github/watchers/VOID404/MongoDB-convert.png?style=social&label=Watch&maxAge=2592000)]()"
    }
  }
}
